/* pages/history/history.wxss */
.container{
  padding: 0rpx;
}
.background {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
}

.link {
  font-family: "STHeiti";
  font-size: 50rpx;
  background:  -webkit-linear-gradient(left,skyblue,deepskyblue,dodgerblue);
  height: 60rpx;
  width: 240rpx;
  align-items: center;
  border-style: ridge;
  border-color: rgb(65, 133, 236);
  padding-left: 31rpx;
  border-top-width:5rpx;
  border-bottom-width:5rpx;
  border-left-width:5rpx;
  border-right-width:5rpx;
}
